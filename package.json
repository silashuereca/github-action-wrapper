{
  "name": "supabase-vue-3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "eslint --ext .ts,.vue src && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue src",
    "lint-fix": "eslint --fix 'src/**/*.{js,ts,vue}'",
    "run-migration": "supabase db diff --use-migra -f",
    "deploy-migration": "supabase db push",
    "watch-functions": "supabase functions serve --watch",
    "create-function": "supabase functions new",
    "deploy-functions": "supabase functions deploy",
    "deploy-all": "supabase functions deploy && supabase db push"
  },
  "dependencies": {
    "@primeuix/themes": "^1.0.0",
    "@supabase/supabase-js": "^2.49.3",
    "@tailwindcss/vite": "^4.0.17",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "buffer": "^6.0.3",
    "luxon": "^3.6.1",
    "octokit": "^4.1.2",
    "pinia": "^3.0.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.3",
    "tailwindcss": "^4.0.17",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.5.3",
    "supabase": "^2.20.12",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.0"
  }
}
